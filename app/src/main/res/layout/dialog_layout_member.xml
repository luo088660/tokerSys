<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        style="@style/style_match_wrap_content"
        android:orientation="vertical">

    <RelativeLayout style="@style/style_match_wrap_content">

        <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_45"
                android:background="#fff6f6f6"
                android:gravity="center"
                android:text="请选择排名类型"
                android:textColor="@color/c_black_7"
                android:textSize="@dimen/dimen_14" />

        <TextView
                android:id="@+id/tv_cancel"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dimen_45"
                android:gravity="center"
                android:padding="@dimen/dimen_10"
                android:text="取消"
                android:textColor="@color/c_black_6"
                android:textSize="@dimen/dimen_14" />

    </RelativeLayout>

    <View style="@style/horizontalLine" />

    <TextView
            android:id="@+id/tv_member_01"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_45"
            android:background="@color/white"
            android:gravity="center"
            android:text="项目"
            android:textColor="@color/c_black_6"
            android:textSize="@dimen/dimen_14" />

    <View style="@style/horizontalLine" />

    <TextView
            android:id="@+id/tv_member_02"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_45"
            android:background="@color/white"
            android:gravity="center"
            android:text="团队"
            android:textColor="@color/c_black_6"
            android:textSize="@dimen/dimen_14" />

    <View style="@style/horizontalLine" />

    <TextView
            android:id="@+id/tv_member_03"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_45"
            android:background="@color/white"
            android:gravity="center"
            android:text="拓客员"
            android:textColor="@color/c_black_6"
            android:textSize="@dimen/dimen_14" />


</LinearLayout>