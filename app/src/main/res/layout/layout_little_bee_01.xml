<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        style="@style/style_match_wrap_content"
        android:orientation="vertical">

    <LinearLayout
            style="@style/style_match_content"
            android:layout_marginLeft="@dimen/dimen_16"
            android:layout_marginRight="@dimen/dimen_16"
            android:orientation="vertical">

        <LinearLayout
                style="@style/style_match_wrap_content"
                android:gravity="center">

            <TextView
                    android:id="@+id/tv_my_cus_03"
                    style="@style/style_wrap_content"
                    android:layout_marginRight="@dimen/dimen_8"
                    android:text="注册日期"
                    android:textSize="@dimen/dimen_12" />

            <LinearLayout
                    style="@style/style_match_wrap_content"
                    android:layout_gravity="center"
                    android:background="@color/c_gray_12"
                    android:gravity="right|center_vertical">

                <TextView
                        android:id="@+id/tv_entire_04"
                        style="@style/style_wrap_content_ts12_tchg6_tcb6_pdtb14_pdlr10"
                        android:hint="任务开始日期" />

                <TextView
                        style="@style/style_wrap_content"
                        android:layout_marginLeft="@dimen/dimen_6"
                        android:layout_marginRight="@dimen/dimen_6"
                        android:text="至"
                        android:textColor="@color/c_gray_6"
                        android:textSize="@dimen/dimen_12" />

                <TextView
                        android:id="@+id/tv_entire_05"
                        style="@style/style_wrap_content_ts12_tchg6_tcb6_pdtb14_pdlr10"
                        android:hint="任务结束日期" />

                <ImageView
                        android:id="@+id/img_entire_05"
                        android:layout_width="@dimen/dimen_30"
                        android:layout_height="@dimen/dimen_30"
                        android:padding="@dimen/dimen_8"
                        android:src="@mipmap/icon_shuaxuan" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
                style="@style/style_match_wrap_content"
                android:layout_marginTop="@dimen/dimen_10">

            <EditText
                    android:id="@+id/tv_my_cus_05"
                    style="@style/style_wrap_content"
                    android:layout_width="@dimen/dimen_0"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:hint="输入姓名/手机号码查询"
                    android:paddingLeft="@dimen/dimen_10"
                    android:paddingTop="@dimen/dimen_5"
                    android:paddingRight="@dimen/dimen_10"
                    android:paddingBottom="@dimen/dimen_5"
                    android:textColor="@color/c_black_6"
                    android:textColorHint="@color/c_black_7"
                    android:textSize="@dimen/dimen_12" />

            <TextView
                    android:id="@+id/tv_my_cus_06"
                    style="@style/style_wrap_content"
                    android:layout_marginLeft="@dimen/dimen_10"
                    android:background="@drawable/btn_bg_quit_login_normal"
                    android:gravity="center"
                    android:paddingLeft="@dimen/dimen_16"
                    android:paddingTop="@dimen/dimen_5"
                    android:paddingRight="@dimen/dimen_16"
                    android:paddingBottom="@dimen/dimen_5"
                    android:text="查询"
                    android:textColor="@color/white_01"
                    android:textSize="@dimen/dimen_12" />

        </LinearLayout>


        <TextView
                android:id="@+id/tv_my_cus_07"
                style="@style/style_match_wrap_content"
                android:layout_centerVertical="true"
                android:gravity="center"
                android:padding="@dimen/dimen_16"
                android:text="共 0 个小蜜蜂"
                android:textSize="@dimen/dimen_14" />
    </LinearLayout>
</LinearLayout>