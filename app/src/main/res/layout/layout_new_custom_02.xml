<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        style="@style/style_match_wrap_content"
        android:orientation="vertical">

    <LinearLayout
            style="@style/style_match_wrap_content"
            android:orientation="vertical">

        <LinearLayout style="@style/style_match_wrap_content_pl16_pr16_bw_gc">

            <TextView
                    android:id="@+id/tv_new_cst_05"
                    style="@style/style_wrap_content_mt15_mb15_w1_ts14"
                    android:text="城市" />

            <TextView
                    android:id="@+id/tv_new_custom2_01"
                    style="@style/style_wrap_content_tch0_tc6_bnull_gr_w1_ts14"
                    android:layout_weight="1"
                    android:drawableRight="@mipmap/icon_xxjt"
                    android:drawablePadding="@dimen/dimen_10"
                    android:hint="请选择城市" />
        </LinearLayout>

        <View style="@style/horizontalLine" />

        <LinearLayout style="@style/style_match_wrap_content_pl16_pr16_bw_gc">

            <TextView
                    android:id="@+id/tv_new_cst_06"
                    style="@style/style_wrap_content_mt15_mb15_w1_ts14"
                    android:text="意向楼盘" />

            <TextView
                    android:id="@+id/btn_new_custom_2"
                    style="@style/style_wrap_content_tch0_tc6_bnull_gr_w1_ts14"
                    android:layout_weight="1"
                    android:drawableRight="@mipmap/icon_xxjt"
                    android:drawablePadding="@dimen/dimen_10"
                    android:ellipsize="end"
                    android:hint="请选择意向楼盘"
                    android:maxEms="10"
                    android:maxLines="1" />

            <TextView
                    android:id="@+id/tv_new_cust_moren"
                    style="@style/style_wrap_content"
                    android:paddingLeft="@dimen/dimen_20"
                    android:text="默认"
                    android:textColor="@color/btn_red"
                    android:textSize="@dimen/dimen_14" />
        </LinearLayout>

        <View style="@style/horizontalLine" />

        <LinearLayout
                android:id="@+id/ll_new_custom_03"
                style="@style/style_match_wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

            <LinearLayout style="@style/style_match_wrap_content_pl16_pr16_bw_gc">

                <TextView
                        style="@style/style_wrap_content_mt15_mb15_w1_ts14"
                        android:layout_width="311dp"
                        android:id="@+id/tv_new_custom_033"
                        android:layout_height="wrap_content"
                        android:text="到访地区" />

                <TextView
                        android:id="@+id/btn_new_custom_3"
                        style="@style/style_wrap_content"
                        android:drawableRight="@mipmap/icon_right"
                        android:drawablePadding="@dimen/dimen_10"
                        android:hint="请选择"
                        android:paddingLeft="@dimen/dimen_10"
                        android:paddingTop="@dimen/dimen_5"
                        android:paddingRight="@dimen/dimen_10"
                        android:paddingBottom="@dimen/dimen_5"
                        android:textColor="@color/c_black_6"
                        android:textSize="@dimen/dimen_12" />
            </LinearLayout>

            <View style="@style/horizontalLine" />

            <LinearLayout style="@style/style_match_wrap_content_pl16_pr16_bw_gc">

                <TextView
                        style="@style/style_wrap_content_mt15_mb15_w1_ts14"
                        android:id="@+id/tv_new_custom_044"
                        android:text="到访地点" />

                <TextView
                        android:id="@+id/btn_new_custom_4"
                        style="@style/style_wrap_content"
                        android:drawableRight="@mipmap/icon_right"
                        android:drawablePadding="@dimen/dimen_10"
                        android:hint="请选择"
                        android:paddingLeft="@dimen/dimen_10"
                        android:paddingTop="@dimen/dimen_5"
                        android:paddingRight="@dimen/dimen_10"
                        android:paddingBottom="@dimen/dimen_5"
                        android:textColor="@color/c_black_6"
                        android:textSize="@dimen/dimen_12" />
            </LinearLayout>

            <View style="@style/horizontalLine" />
        </LinearLayout>

        <LinearLayout style="@style/style_match_wrap_content_pl16_pr16_bw_gc">

            <TextView
                    android:id="@+id/tv_new_cst_07"
                    style="@style/style_wrap_content_mt15_mb15_w1_ts14"
                    android:text="预计看房日期" />

            <TextView
                    android:id="@+id/btn_new_custom_5"
                    style="@style/style_wrap_content_tch0_tc6_bnull_gr_w1_ts14"
                    android:layout_weight="1"
                    android:drawableRight="@mipmap/icon_xxjt"
                    android:drawablePadding="@dimen/dimen_10"
                    android:hint="请选择看房日期" />
        </LinearLayout>

        <View style="@style/horizontalLine" />

    </LinearLayout>
</LinearLayout>