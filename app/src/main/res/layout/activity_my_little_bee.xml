<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        style="@style/style_match_content"
        android:background="@color/c_gray_12"
        android:fitsSystemWindows="true"
        android:orientation="vertical">

    <include layout="@layout/layout_content_title" />

    <LinearLayout
            style="@style/style_match_wrap_content"
            android:visibility="gone"
            android:background="@color/white_01">

        <LinearLayout
                style="@style/style_wrap_content"
                android:layout_width="@dimen/dimen_0"
                android:gravity="center"
                android:layout_weight="1">

            <TextView
                    style="@style/style_wrap_content"
                    android:drawableRight="@mipmap/icon_xxjt"
                    android:drawablePadding="@dimen/dimen_10"
                    android:padding="@dimen/dimen_16"
                    android:text="清远银湖城"
                    android:textColor="@color/c_black_6"
                    android:textSize="@dimen/dimen_14" />
        </LinearLayout>

        <LinearLayout
                style="@style/style_wrap_content"
                android:layout_width="@dimen/dimen_0"
                android:id="@+id/ll_my_little"
                android:gravity="center"
                android:layout_weight="1">

            <TextView
                    android:id="@+id/tv_entire_05"
                    style="@style/style_wrap_content_ts12_tchg6_tcb6_pdtb14_pdlr10"
                    android:drawablePadding="@dimen/dimen_10"
                    android:textSize="@dimen/dimen_14"
                    android:drawableRight="@mipmap/icon_shuaxuan"
                    android:hint="任务结束日期" />
        </LinearLayout>
    </LinearLayout>

    <View style="@style/horizontalLine_16" />

    <android.support.v7.widget.RecyclerView
            android:id="@+id/rv_my_been"
            style="@style/style_match_wrap_content"
            android:layout_marginBottom="@dimen/dimen_1"
            android:background="@color/white_01" />

    <FrameLayout
            android:id="@+id/fl_my_bee"
            style="@style/style_match_content" />
</LinearLayout>