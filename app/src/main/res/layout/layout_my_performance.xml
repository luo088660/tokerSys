<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        style="@style/style_match_wrap_content"
        android:background="@color/c_gray_12"
        android:orientation="vertical">

    <LinearLayout
            style="@style/style_match_wrap_content"
            android:layout_marginBottom="@dimen/dimen_10"
            android:orientation="vertical">

        <LinearLayout
                style="@style/style_match_wrap_content"
                android:layout_marginTop="@dimen/dimen_10"
                android:background="@color/white_01"
                android:gravity="center"
                android:paddingRight="@dimen/dimen_16">

            <TextView
                    style="@style/style_wrap_content"
                    android:layout_margin="@dimen/dimen_16"
                    android:layout_weight="1"
                    android:text="我的业绩"
                    android:textColor="@color/c_black_6"
                    android:textSize="@dimen/dimen_16"
                    android:textStyle="bold" />

            <LinearLayout
                    style="@style/style_wrap_content"
                    android:id="@+id/ll_my_performance_01"
                    android:gravity="center"
                    android:orientation="horizontal">

                <TextView
                        style="@style/style_wrap_content_ts12_tchg6_tcb6_pdtb14_pdlr10"
                        android:gravity="center"
                        android:hint="日期"
                        android:id="@+id/tv_my_performance_01"
                        android:text="2019/06/01" />

                <ImageView
                        android:layout_width="@dimen/dimen_30"
                        android:layout_height="@dimen/dimen_30"
                        android:padding="@dimen/dimen_8"
                        android:src="@mipmap/icon_shuaxuan" />
            </LinearLayout>
        </LinearLayout>

        <TextView
                android:id="@+id/tv_ver_01"
                style="@style/style_wrap_content_w0_gc_pd14_ts12_tcb6_drxx"
                android:layout_width="wrap_content"
                android:layout_gravity="center"
                android:drawablePadding="@dimen/dimen_5"
                android:text="全部项目" />


        <include layout="@layout/layout_perfor" />

    </LinearLayout>

</LinearLayout>