<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              style="@style/style_match_content"
              android:background="@color/c_gray_12"
              android:fitsSystemWindows="true"
              android:orientation="vertical">
    <include layout="@layout/layout_content_title"/>

    <LinearLayout style="@style/style_match_wrap_content"
                  android:layout_margin="@dimen/dimen_15"
                  android:orientation="vertical">
        <TextView style="@style/style_match_wrap_content"
                  android:text="按指标到访率"
                  android:background="@drawable/txt_bg_selector"
                  android:layout_marginRight="@dimen/dimen_8"
                  android:padding="@dimen/dimen_10"
                  android:drawableRight="@mipmap/icon_xxjt"/>
        <TextView style="@style/style_match_wrap_content"
                  android:text="大于40%"
                  android:layout_marginTop="@dimen/dimen_10"
                  android:background="@drawable/txt_bg_selector"
                  android:layout_marginRight="@dimen/dimen_8"
                  android:padding="@dimen/dimen_10"
                  android:drawableRight="@mipmap/icon_xxjt"/>


        <TextView style="@style/style_match_wrap_content"
                  android:background="@drawable/btn_bg_quit_login_normal"
                  android:textColor="@color/white_01"
                  android:gravity="center"
                android:paddingLeft="@dimen/dimen_16"
                android:paddingTop="@dimen/dimen_5"
                android:paddingRight="@dimen/dimen_16"
                android:paddingBottom="@dimen/dimen_5"
                  android:layout_marginTop="@dimen/dimen_18"
                  android:textSize="@dimen/dimen_14"
                  android:text="推荐"/>
    </LinearLayout>
    <FrameLayout style="@style/style_match_content">
        <com.toker.sys.utils.view.MyMapView
                style="@style/style_match_wrap_content">

        <com.baidu.mapapi.map.TextureMapView
                android:id="@+id/mv_map"
                style="@style/style_match_content"/>
        </com.toker.sys.utils.view.MyMapView>
    </FrameLayout>

</LinearLayout>