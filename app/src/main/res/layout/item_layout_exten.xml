<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:background="@color/white"
              style="@style/style_match_wrap_content">

    <LinearLayout style="@style/style_match_wrap_content"
                  android:padding="@dimen/dimen_15">
        <TextView style="@style/style_wrap_content"
                  android:textSize="@dimen/dimen_12"
                  android:textColor="@color/c_black_6"
                  android:id="@+id/tv_item_exten_01"
                  android:layout_width="@dimen/dimen_0"
                  android:gravity="center"
                  android:layout_weight="1"
                  android:text="姓名"/>

        <TextView style="@style/style_wrap_content"
                  android:layout_weight="2"
                  android:textSize="@dimen/dimen_12"
                  android:textColor="@color/c_black_6"
                  android:layout_width="@dimen/dimen_0"
                  android:id="@+id/tv_item_exten_02"
                  android:gravity="center"
                  android:text="手机号码"/>

        <TextView style="@style/style_wrap_content"
                  android:layout_weight="1"
                  android:textSize="@dimen/dimen_12"
                  android:layout_width="@dimen/dimen_0"
                  android:id="@+id/tv_item_exten_03"
                  android:textColor="@color/c_black_6"
                  android:gravity="center"
                  android:text="拓客岗位"/>

        <TextView style="@style/style_wrap_content"
                  android:layout_weight="2"
                  android:textSize="@dimen/dimen_12"
                  android:id="@+id/tv_item_exten_04"
                  android:maxLength="6"
                  android:layout_width="@dimen/dimen_0"
                  android:maxEms="6"
                  android:ellipsize="end"
                  android:textColor="@color/c_black_6"
                  android:gravity="center"
                  android:text="归属拓客团队"/>
    </LinearLayout>

    <View style="@style/horizontalLine"/>

</LinearLayout>