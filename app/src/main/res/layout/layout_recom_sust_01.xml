<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        style="@style/style_match_wrap_content"
        android:background="@color/c_gray_12"
        android:orientation="vertical">

    <LinearLayout
            style="@style/style_match_wrap_content"
            android:gravity="center"
            android:paddingRight="@dimen/dimen_16">

        <TextView
                android:id="@+id/tv_receivde1_02"
                style="@style/style_wrap_content_w0_gc_pd14_ts12_tcb6_drxx"
                android:text="全部项目" />

        <TextView
                android:id="@+id/tv_receivde1_03"
                style="@style/style_wrap_content_w0_gc_pd14_ts12_tcb6_drxx"
                android:text="全部状态" />

        <LinearLayout
                android:id="@+id/ll_receivde1_04"
                style="@style/style_wrap_content"
                android:gravity="center"
                android:orientation="horizontal">

            <TextView
                    android:id="@+id/tv_receivde1_04"
                    style="@style/style_wrap_content_ts12_tchg6_tcb6_pdtb14_pdlr10"
                    android:hint="开始时间\n结束时间" />

            <ImageView
                    android:layout_width="@dimen/dimen_30"
                    android:layout_height="@dimen/dimen_30"
                    android:padding="@dimen/dimen_8"
                    android:src="@mipmap/icon_shuaxuan" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
            style="@style/style_match_wrap_content"
            android:background="@color/c_gray_122"
            android:orientation="vertical"
            android:paddingLeft="@dimen/dimen_16"
            android:paddingTop="@dimen/dimen_16"
            android:paddingRight="@dimen/dimen_16">


        <LinearLayout style="@style/style_match_wrap_content">

            <EditText
                    android:id="@+id/tv_receivde1_01"
                    style="@style/style_wrap_content"
                    android:layout_width="@dimen/dimen_0"
                    android:layout_weight="1"
                    android:background="@drawable/txt_bg_selector"
                    android:hint="输入姓名/手机号码查询"

                    android:paddingLeft="@dimen/dimen_10"
                    android:paddingTop="@dimen/dimen_5"
                    android:paddingRight="@dimen/dimen_10"
                    android:paddingBottom="@dimen/dimen_5"
                    android:textColor="@color/c_black_6"
                    android:textColorHint="@color/c_black_7"
                    android:textSize="@dimen/dimen_12" />

            <TextView
                    android:id="@+id/btn_receivde1_01"
                    style="@style/style_wrap_content"
                    android:layout_marginLeft="@dimen/dimen_10"
                    android:background="@drawable/btn_bg_quit_login_normal"
                    android:gravity="center"
                    android:paddingLeft="@dimen/dimen_16"
                    android:paddingTop="@dimen/dimen_5"
                    android:paddingRight="@dimen/dimen_16"
                    android:paddingBottom="@dimen/dimen_5"
                    android:text="查询"
                    android:textColor="@color/white_01"
                    android:textSize="@dimen/dimen_12" />

        </LinearLayout>

        <TextView
                android:id="@+id/tv_receivde1_07"
                style="@style/style_match_wrap_content"
                android:layout_centerVertical="true"
                android:gravity="left"
                android:paddingTop="@dimen/dimen_10"
                android:paddingBottom="@dimen/dimen_10"
                android:text="共 0 个任务"
                android:textSize="@dimen/dimen_12" />

    </LinearLayout>

</LinearLayout>